def createNowNode()
  thissecond = Second.create
  
  timenow = Time.now
  thissecond.unixtime = timenow
  thissecond.second = timenow.strftime "%Y%m%d_%H%M%S%z"
  thissecond.name = thissecond.second
  thissecond.save
end

